<script
	src="https://www.google.com/recaptcha/enterprise.js?render=6LdukTcqAAAAADjnv-dv4OZ4GoffcT8oMqnlNQJ0"></script>
<script src="https://www.google.com/recaptcha/api.js"></script>

<div id="canvas">
    <div id="box_wrapper">
        <!-- template sections -->
        <section class="ls section_padding_top_100 section_padding_bottom_100 section_full_height">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4 to_animate">
                        <div class="with_border with_padding">
                            <h4 class="text-center">
                                Sign Up New Account
                            </h4>
                            <hr class="bottommargin_30">
                            <div class="wrap-forms">
                                <form id="dr-sign-up-form" action="/dr/create" method="post">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="first-name">First Name</label>
                                                <i class="grey fa fa-user"></i>
                                                <input type="text" class="form-control" id="first-name" name="firstName" placeholder="First Name">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="last-name">Last Name</label>
                                                <i class="grey fa fa-user"></i>
                                                <input type="text" class="form-control" id="last-name" name="lastName" placeholder="Last Name" >
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="login-email">Email address</label>
                                                <i class="grey fa fa-envelope-o"></i>
                                                <input type="email" class="form-control" id="login-email" name="email" placeholder="Email" >
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="phone">Phone</label>
                                                <i class="grey fas fa-mobile-alt"></i>
                                                <input type="tel" class="form-control" id="login-phone" name="phone" pattern="[0-9]{10}" inputmode="numeric" maxlength="10" placeholder="Mobile">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="specilization">specialization</label>
                                                <i class="grey fas fa-briefcase-medical"></i>
                                                <input type="text" class="form-control" id="specialization" name="specialization" placeholder="Specialization" >
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="license">License Number</label>
                                                <i class="grey fas fa-id-card"></i>
                                                <input type="tel" class="form-control" id="license-num" name="license_num" pattern="[0-9]{10}" inputmode="numeric" maxlength="10" placeholder="License Number">
                                            </div>
                                        </div>
                                    </div>
                                
                                    <!-- set password section -->
                                    <div class="row ">
                                        <div class="col-sm-12" >
                                            <h4 class="text-center" style="padding-top:15px ;">Set Password</h4>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="login-password">Password</label>
                                                <i class="grey fa fa-pencil-square-o"></i>
                                                <input type="password" class="form-control" id="login-password" name="password" placeholder="Password">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="login-password2">Retype Password</label>
                                                <i class="grey fa fa-pencil-square-o"></i>
                                                <input type="password" class="form-control" id="login-password2" placeholder="Retype Password" >
                                            </div>
                                        </div>
                                    </div>
                                    <!-- Address Section -->
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <h4 class="text-center" style="padding-top:15px ;">Address</h4>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="street">Street</label>
                                                <i class="grey fa fa-map-marker"></i>
                                                <input type="text" class="form-control" id="street" name="address[street]" placeholder="Street">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="city">City</label>
                                                <i class="grey fa fa-building"></i>
                                                <input type="text" class="form-control" id="city" name="address[city]" placeholder="City" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="state">State</label>
                                                <i class="grey fa fa-map"></i>
                                                <input type="text" class="form-control" id="state" name="address[state]" placeholder="State" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="zip-code">Zip Code</label>
                                                <i class="grey fa fa-envelope"></i>
                                                <input type="text" class="form-control" id="zip-code" name="address[zipCode]" maxlength="6" placeholder="Zip Code" >
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <label for="country">Country</label>
                                                <i class="grey fa fa-globe"></i>
                                                <input type="text" class="form-control" id="country" name="address[country]" placeholder="Country" >
                                            </div>
                                        </div>
                                    </div>
                                   
                                
                                    <!-- hidden recaptcha input -->
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group has-placeholder">
                                                <input type="hidden" id="g-recaptcha" name="recaptcha">
                                            </div>
                                        </div>
                                    </div>
                                
                                    <button type="submit" id="submit-button" class="theme_button block_button color1 g-recaptcha" 
                                    data-sitekey="6LdukTcqAAAAADjnv-dv4OZ4GoffcT8oMqnlNQJ0"  onclick="onClick(event)"
                                     data-action='submit'>Create an account</button>
                                </form>
                            </div>
                        </div>
                        <!-- .with_border -->
                        <p class="divider_20 text-center">
                            Already registered?
                            <a href="/dr-sign-in">Log In</a>
                        </p>
                    </div>
                    <!-- .col-* -->
                </div>
                <!-- .row -->
            </div>
            <!-- .container -->
        </section>
    </div>
    <!-- eof #box_wrapper -->
</div>

<script src="/js/mindcare/dr_sign_up.js"></script>